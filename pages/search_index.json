[
{% assign docs = site.items | sort: 'title' %}
{% for doc in docs %}
{
  "id": "{{ doc.url | relative_url }}",
  "title": {{ doc.title | jsonify }},
  "url": "{{ doc.url | relative_url }}",
  "link": "{{ doc.url | relative_url }}",
  "date": {{ doc.date | jsonify }},
  "source": {{ doc.source | jsonify }},
  "tags": {{ doc.tags | jsonify }},
  "categories": {{ doc.tags | jsonify }},
  "content": {{ doc.content | strip_html | normalize_whitespace | jsonify }},
  "body": {{ doc.content | strip_html | normalize_whitespace | jsonify }}
}{% unless forloop.last %},{% endunless %}
{% endfor %}
]